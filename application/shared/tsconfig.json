{
    "compilerOptions": {
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "allowSyntheticDefaultImports": true,
        "sourceMap": true,
        "removeComments": false,
        "preserveConstEnums": true,
        "strictNullChecks": false,
        "experimentalDecorators": true
    },
    "exclude": [
        "node_modules",
        "build",
        "dist",
        ".awcache"
    ]
}
